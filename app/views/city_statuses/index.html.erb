<p id="notice"><%= notice %></p>

<h1>City Statuses</h1>

<table>
  <thead>
    <tr>
      <th>City</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @city_statuses.each do |city_status| %>
      <tr>
        <td><%= city_status.city_id %></td>
        <td><%= city_status.status %></td>
        <td><%= link_to 'Show', city_status %></td>
        <td><%= link_to 'Edit', edit_city_status_path(city_status) %></td>
        <td><%= link_to 'Destroy', city_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New City Status', new_city_status_path %>
